{
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": false,
  },
  "css.validate": false,
  "scss.validate": false,
  "stylelint.autoFixOnSave": true,
  "typescript.validate.enable": false
}
